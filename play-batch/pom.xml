    <dependencies>
        <dependency>
            <groupId>dalgun.github.io</groupId>
            <artifactId>play-commons</artifactId>
            <version>0.0.1-SNAPSHOT</version>
        </dependency>
        <dependency>
            <groupId>org.springframework.boot</groupId>
